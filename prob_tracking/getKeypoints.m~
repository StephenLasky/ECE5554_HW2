function [keyXs, keyYs] = getKeypoints(im, tau)
% Detecting keypoints using Harris corner criterion  

% im: input image
% tau: threshold

% keyXs, keyYs: detected keypoints, with dimension [Number of keypoints] x [2]

im = KLT_tracker(im, tau);
num_keypoints = 0;
keyXs = zeros(1,0);
keyYs = zeros(1,0);

[rows,cols] = size(im);

  
end